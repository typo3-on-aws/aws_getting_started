{
	"name": "typo3-on-aws/aws-getting-started",
	"type": "typo3-cms-extension",
	"description": "Simple start screen with basic information for the TYPO3-on-AWS machine image.",
	"homepage": "https://t3rrific.com/typo3-on-aws/",
	"authors": [
		{
			"name": "Michael Schams",
			"role": "Developer"
		}
	],
	"config": {
		"sort-packages": true
	},
	"require": {
		"typo3/cms-core": "^10 || ^11"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "aws_getting-started"
		}
	},
	"autoload": {
		"psr-4": {
			"Typo3OnAws\\AwsGettingStarted\\": "Classes"
		}
	},
	"replace": {
		"aws_getting_started": "self.version",
		"typo3-ter/aws-getting-started": "self.version"
	}
}
